{
    "compilerOptions": {
        "target": "es6",
        "module": "NodeNext",
        "moduleResolution": "node",
        "isolatedModules": true,
        "strict": true,
        "baseUrl": "src",
        "paths": {
            "@middleware/*": [
                "middleware/*"
            ],
            "@models/*": [
                "models/*"
            ],
        }
    },
    "include": [
        "src/**/*",
        "src/index.js"
    ],
    "exclude": [
        "node_modules",
    ]
}